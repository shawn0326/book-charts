            "data": [{
                "name": "《儒林外史》",
                "draggable": true,
                "des": ""
            }, {
                "name": "王冕",
                "category": 0,
                "draggable": true,
                "des": "浙江诸暨县、乡村。"
            }, {
                "name": "梅玖",
                "category": 1,
                "draggable": true,
                "des": "山东兖州府汶上县薛家集，秀才。"
            }, {
                "name": "周进",
                "category": 0,
                "draggable": true,
                "des": "山东兖州府汶上县，进士，官至国子监司业。"
            }, {
                "name": "王惠",
                "category": 0,
                "draggable": true,
                "des": "山东兖州府汶上县，举人，官至南昌府知府，后降了宁王。"
            }, {
                "name": "荀玫",
                "category": 1,
                "draggable": true,
                "des": "山东兖州府汶上县薛家集人，进士，官至工部主事。"
            }, {
                "name": "范进",
                "category": 0,
                "draggable": true,
                "des": "南海人，进士，官至山东学道。"
            }, {
                "name": "张乡绅",
                "category": 0,
                "draggable": true,
                "des": "南海人，举人，做过一任知县，别号静斋。"
            }, {
                "name": "汤知县",
                "category": 0,
                "draggable": true,
                "des": "高要县知县。"
            }, {
                "name": "严大位",
                "category": 0,
                "draggable": true,
                "des": "字致中，高要县人，贡生，无恶不作。"
            }, {
                "name": "严大育",
                "category": 0,
                "draggable": true,
                "des": "字致和，高要县人，监生。"
            }, {
                "name": "王氏",
                "category": 1,
                "draggable": true,
                "des": "严大育的妻子，病亡。"
            }, {
                "name": "赵氏",
                "category": 1,
                "draggable": true,
                "des": "严大育妾，王氏死后扶正。"
            }, {
                "name": "王德",
                "category": 1,
                "draggable": true,
                "des": "高要县人，府学廪膳生员，坐馆教学。"
            }, {
                "name": "王仁",
                "category": 1,
                "draggable": true,
                "des": "高要县人，县学廪膳生员，坐馆教学。"
            }, {
                "name": "陈礼",
                "category": 1,
                "draggable": true,
                "des": "字和甫，江西南昌县人，善扶乩，行走在高级官吏家中。"
            }, {
                "name": "娄家二公子",
                "category": 0,
                "draggable": true,
                "des": "娄三公子：娄奉，字玉亭，湖州人，孝廉。喜结交名士。组织鶯脰会。娄四公子：娄瓒，字瑟亭，湖州人，在监读书。"
            }, {
                "name": "杨执中",
                "category": 1,
                "draggable": true,
                "des": "湖州新市镇东四里的人，廪生俟贡，喜读书。做了娄府宾客。"
            }, {
                "name": "张铁臂",
                "category": 1,
                "draggable": true,
                "des": "杨执中引入娄府的宾客，实为骗子。"
            }, {
                "name": "鲁编修",
                "category": 1,
                "draggable": true,
                "des": "湖州人，在京城翰林院供职，后告假还乡。"
            }, {
                "name": "蘧冼夫",
                "category": 0,
                "draggable": true,
                "des": "蘧公孙，嘉兴人。秀才，后入赘鲁编修家女婿。"
            }, {
                "name": "鲁小姐",
                "category": 1,
                "draggable": true,
                "des": "湖州人，蘧公孙的妻子，从小喜欢读八股文，脑子里记得三千篇文章。"
            }, {
                "name": "权勿用",
                "category": 1,
                "draggable": true,
                "des": "字潜斋，浙江萧山县人，老童生。做了娄府一段时间宾客。"
            }, {
                "name": "马静",
                "category": 0,
                "draggable": true,
                "des": "马纯上，人称马二先生，浙江处州人。秀才，老廪生，选家。"
            }, {
                "name": "洪憨仙",
                "category": 1,
                "draggable": true,
                "des": "台州人，炼丹，在杭州伍相国庙住着以炼丹术骗人。"
            }, {
                "name": "胡三公子",
                "category": 0,
                "draggable": true,
                "des": "胡缜，字密之，杭州人。尚书后代，比较富有。"
            }, {
                "name": "李本瑛",
                "category": 1,
                "draggable": true,
                "des": "乐清县知县，被冤平反后进京赴任。"
            }, {
                "name": "匡迥",
                "category": 0,
                "draggable": true,
                "des": "字超人，浙江温州乐清县人。贡生，后到京城当了国子监教习。"
            }, {
                "name": "匡大",
                "category": 1,
                "draggable": true,
                "des": "匡迥的哥哥，自私吝啬。"
            }, {
                "name": "潘三",
                "category": 1,
                "draggable": true,
                "des": "杭州的‘黑社会大哥’。把持官府，包揽词讼，拐带人口，买嘱枪手。对匡迥讲义气，事事帮衬。后入狱。"
            }, {
                "name": "景本蕙",
                "category": 1,
                "draggable": true,
                "des": "景蘭江，温州人，小商人，在杭州城开头巾店，喜欢吟诗、写诗。"
            }, {
                "name": "金东崖",
                "category": 1,
                "draggable": true,
                "des": "绍兴府会稽人。吏部掌案，著有《四书讲章》，花五百两银子找抢手替儿子考秀才。"
            }, {
                "name": "赵洁",
                "category": 1,
                "draggable": true,
                "des": "赵雪斋，杭州人，秀才，喜欢写诗。"
            }, {
                "name": "支剑锋",
                "category": 1,
                "draggable": true,
                "des": "（麻子）杭城诗会中领袖。任分府盐务巡商。"
            }, {
                "name": "浦玉方",
                "category": 1,
                "draggable": true,
                "des": "浦墨卿（胡子）杭城诗会中领袖，生员。"
            }, {
                "name": "郑老爹",
                "category": 1,
                "draggable": true,
                "des": "抚院大人家里当差。"
            }, {
                "name": "郑家三女儿",
                "category": 1,
                "draggable": true,
                "des": "相貌端正。匡迥进京赴任，被遣至浙江温州乐清县乡下老家，不久病亡。"
            }, {
                "name": "牛布衣",
                "category": 0,
                "draggable": true,
                "des": "绍兴人，诗人，在山东范学道身边做过幕客，后病死在芜湖甘露庵。"
            }, {
                "name": "甘露寺和尚",
                "category": 0,
                "draggable": true,
                "des": "甘露寺的老和尚。与暂住在寺中的牛布衣交好，牛布衣死后，为牛布衣料理丧事，情真意切。后又收留牛浦郎在大殿中读诗。后被请到'京里报国寺去做方丈'，临行前将牛布衣留下的两部诗集赠与牛浦郎。"
            }, {
                "name": "牛浦郎",
                "category": 0,
                "draggable": true,
                "des": "芜湖人。从甘露庵和尚处得到牛布衣的诗稿，便自称牛布衣和名士来往。"
            }, {
                "name": "牛瑶",
                "category": 0,
                "draggable": true,
                "des": "字玉圃，徽州人，在扬州大盐商万家吃嘴皮子饭，后被赶走。"
            }, {
                "name": "万雪斋",
                "category": 1,
                "draggable": true,
                "des": "扬州大盐商。出身微贱，后暴富。"
            }, {
                "name": "董知县",
                "category": 0,
                "draggable": true,
                "des": "误认牛瑶为'才子'牛布衣，升任别处后还托付向知县照顾牛瑶，引得后来向知县处理牛瑶一案有失偏颇，险些被罢官。辛得当时在崔按察做门人的鲍文卿求情才幸免。"
            }, {
                "name": "向知县",
                "category": 0,
                "draggable": true,
                "des": "因鲍文卿对自己有恩，十分照顾。介绍王管家的女儿给鲍庭玺成家，后王管家的女儿病亡。升任后，赠与鲍文卿一千两银子。鲍文卿死后，十分悲痛。"
            }, {
                "name": "鲍文卿",
                "category": 0,
                "draggable": true,
                "des": "南京水西门，做过安庆崔按察的门人，家中几代戏行。后来自己成立了戏班子，经常在杜家兄弟家活动。"
            }, {
                "name": "鲍庭玺",
                "category": 0,
                "draggable": true,
                "des": "鲍文卿的过继儿子，先娶王管家女儿，生孩子死了，又娶胡家女儿。"
            }, {
                "name": "倪霜峰",
                "category": 1,
                "draggable": true,
                "des": "读到秀才，穷困潦倒，修补乐器度日。六个儿子死了一个，卖了四个，剩下一个倪庭玺过继给鲍文卿。"
            }, {
                "name": "季佳",
                "category": 0,
                "draggable": true,
                "des": "字苇萧，安庆府怀宁县人，考过童生案首，和慎卿、少卿相交甚好，和慎卿定梨园榜。后到京城作了高官的幕客。"
            }, {
                "name": "季恬逸",
                "category": 0,
                "draggable": true,
                "des": "安庆人，跟着名人噌饭吃，自己没能力。"
            }, {
                "name": "辛东之",
                "category": 1,
                "draggable": true,
                "des": "扬州名士。后到南京，经常参与名士们的活动。"
            }, {
                "name": "金寓刘",
                "category": 1,
                "draggable": true,
                "des": "扬州名士。后到南京，经常参与名士们的活动。"
            }, {
                "name": "萧鼎",
                "category": 1,
                "draggable": true,
                "des": "字金铉，安庆人，选家 。在南京被诸葛天申雇用给选文章。"
            }, {
                "name": "诸葛佑",
                "category": 0,
                "draggable": true,
                "des": "字天申，江苏盱眙县人。在南京花银子让萧金铉给选文章刻印。"
            }, {
                "name": "郭铁笔",
                "category": 1,
                "draggable": true,
                "des": "芜湖县人，喜欢刻图章，在南京名士间活动。"
            }, {
                "name": "杜倩",
                "category": 0,
                "draggable": true,
                "des": "字慎卿，天长县人。在南京娶了妾，组织莫愁湖大会，后入了贡，到京城参加乡试。"
            }],
